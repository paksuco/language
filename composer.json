{
  "name": "paksuco/language",
  "description": "A complete language package - WIP",
  "keywords": [
    "laravel",
    "language",
    "backend",
    "frontend"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Taha PAKSU",
      "email": "tpaksu@gmail.com",
      "homepage": "https://rettica.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "^7.0",
    "illuminate/config": "^7.0",
    "illuminate/routing": "^7.0",
    "livewire/livewire": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Paksuco\\Language\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Paksuco\\Language\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Paksuco\\Language\\LanguageServiceProvider"
      ],
      "aliases": {
        "Package": "Paksuco\\Language\\Facade"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}